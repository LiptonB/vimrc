" Auto install vundle
if !isdirectory(expand("~/.vim/bundle/vundle"))
    !mkdir -p ~/.vim/bundle
    !git clone git://github.com/gmarik/vundle.git ~/.vim/bundle/vundle
    let s:bootstrap=1
endif


" Set the plugin groups to use
"
" Default
"let g:spf13_bundle_groups=['general', 'neocomplcache', 'programming', 'php', 'ruby', 'python', 'javascript', 'html', 'misc', 'scala']

let g:spf13_bundle_groups_bundle_groups=['general', 'programming', 'javascript', 'html', 'misc']

" SPF13 conflicts with the default digraph mapping of <C-K>
let g:spf13_no_neosnippet_expand = 1

